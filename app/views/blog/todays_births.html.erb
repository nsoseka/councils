<h1>Todays Birth Statistics</h1>
<% if @mean != 0 %>
  <h3 class="nothing">Area Chart</h3>
  <%= area_chart @data, download: true %>
  <h3 class="nothing">Column Chart</h3>
  <%= column_chart @data, download: true %>
  <h3 class="nothing">Pie Chart</h3>
  <%= pie_chart @data, download: true %>
<% else %>
  <h3 class="nothing">No births registered so far today</h3>
<% end %>
