<h1><%= t('.search_results') %></h1>
<div class="find-dc">
  <%= render 'search_form', { search_query: @search_query } %>
</div>
<ul class="children search-results">
   <% if @births.empty? && @new_borns.empty? %>
    <h3><%= t('.no_results') %></h3>
  <% else %>
    <% if @pagy.count >= @pagy2.count %>
      <%== pagy_nav(@pagy) %>
    <% else %>
      <%== pagy_nav(@pagy2) %>
    <% end %>
    <% @births.each do|birth|%>
      <%= render "birth", {birth: birth} %>
    <% end %>
    <% @new_borns.each do|new_born|%>
      <%= render "new_borns/new_born", {new_born: new_born} %>
    <% end %>
  <% end %>
</ul>

